{
  "name": "google-maps-markup",
  "version": "3.2.1",
  "description": "Ember markup component for Google Maps - Draw and Measure",
  "keywords": [
    "ember-addon",
    "google maps",
    "markup",
    "draw",
    "measure"
  ],
  "license": "ISC",
  "author": "Ilya Radchenko",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "git@github.com:knownasilya/google-maps-markup.git",
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "pretest": "mkdir coverage",
    "release": "standard-version"
  },
  "dependencies": {
    "ember-cli-babel": "^7.0.0",
    "ember-cli-htmlbars": "^4.0.0",
    "ember-composability-tools": "~0.0.12",
    "ember-drag-drop": "^0.8.1",
    "ember-palette": "^3.0.0",
    "ember-power-select": "^4.0.0",
    "ember-uuid": "^2.0.0",
    "ember-wormhole": "^0.5.5",
    "ember-copy": "^1.0.0"
  },
  "devDependencies": {
    "@ember/jquery": "^1.1.0",
    "@ember/optional-features": "^1.3.0",
    "babel-eslint": "^10.1.0",
    "bootstrap": "^3.4.1",
    "broccoli-asset-rev": "^3.0.0",
    "broccoli-funnel": "^3.0.2",
    "broccoli-merge-trees": "^4.2.0",
    "coveralls": "^3.0.0",
    "ember-auto-import": "^1.5.3",
    "ember-cli": "~3.17.0",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-github-pages": "^0.2.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-less": "^1.5.3",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resolver": "^7.0.0",
    "ember-source": "~3.17.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-template-lint": "^2.4.0",
    "ember-try": "^1.4.0",
    "eslint": "^6.8.0",
    "eslint-plugin-ember": "^7.10.1",
    "eslint-plugin-node": "^11.0.0",
    "font-awesome": "^4.7.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.1.0",
    "standard-version": "^8.0.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://knownasilya.github.io/google-maps-markup/"
  }
}
